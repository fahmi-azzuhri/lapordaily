datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id               Int                @id @default(autoincrement())
  username         String             @unique
  password         String
  role             String
  laporanPekerjaan LaporanPekerjaan[]
}

model LaporanPekerjaan {
  id             Int      @id @default(autoincrement())
  nama           String
  jenisPekerjaan String
  hasil          String
  tanggal        DateTime
  userId         Int
  user           User     @relation(fields: [userId], references: [id])
}

enum Hasil {
  KG
  PALET
  BOX
}

enum Role {
  USER
  ADMIN
}
