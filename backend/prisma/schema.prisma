datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model LaporanPekerjaan {
  id             Int      @id @default(autoincrement())
  nama           String
  jenisPekerjaan String
  hasil          Hasil
  tanggal        DateTime @default(now())
}

enum Hasil {
  KG
  PALET
  BOX
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  role     Role
}

enum Role {
  USER
  ADMIN
}
